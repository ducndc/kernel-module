D_MOD="drwxr-xr-x"
F_MOD="-rw-r--r--"
S_MOD="lrwxrwxrwx"
DUCNDC_FS_MAX_BLOCKS_PER_EXTENT=8
DUCNDC_FS_BLOCK_SIZE=4096
DUCNDC_FS_FILES_PER_BLOCK=15
DUCNDC_FS_MAX_EXTENTS=255 # $(( ($DUCNDC_FS_BLOCK_SIZE - 4) / 16 ))
MAXFILESIZE=$(( $DUCNDC_FS_MAX_EXTENTS * $DUCNDC_FS_MAX_BLOCKS_PER_EXTENT * $DUCNDC_FS_BLOCK_SIZE ))
MAXFILES=$(( $DUCNDC_FS_MAX_EXTENTS * $DUCNDC_FS_MAX_BLOCKS_PER_EXTENT * $DUCNDC_FS_FILES_PER_BLOCK )) # 36000
MOUNT_TEST=100
